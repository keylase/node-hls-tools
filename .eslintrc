{
    "env": {
        "node": true
    },
    "rules": {
        "quotes": 0,
        "no-comma-dangle": 0,
        "no-mixed-requires": [0, true],
        "curly": [0, "multi"],
        "no-unused-vars": 1,
        "camelcase": 1,
        "no-use-before-define": 1,
        "no-underscore-dangle": 0
    }
}